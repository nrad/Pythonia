<?xml version="1.0" encoding="UTF-8"?><modul xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="modul.xsd" name="SineVco" description="Sine oscillator (amplitude 1)">  <input>    <vardef name="frq" csType="k" min="0" description="Frequency of oscillator"/>  </input>  <output>    <vardef name="out" csType="a" description="Output of oscillator (amplitude 1)"/>  </output>  <global>    <def description="Sine table">gisinevco   ftgen  0, 0, 4096, 10, 1</def>  </global>  <opcode>/* Sine oscillator */opcode SineVco, a, k  kfrq xin  kfrq     limit      kfrq, 0, 50000  asig     oscil      1, kfrq, gisinevco  xout asigendop  </opcode></modul>