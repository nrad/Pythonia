<?xml version="1.0" encoding="UTF-8"?><modul xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="modul.xsd" name="SawVco" description="Saw oscillator (aplitude 1)">  <input>    <vardef name="frq" csType="k" min="0" description="Frequency of oscillator"/>    <vardef name="shape" csType="k" min="0.001" max="0.999" value="0.001" description="Shape of wave, sawtooth/triangle/ramp"/>  </input>  <output>    <vardef name="out" csType="a" description="Output of oscillator (amplitude 1)"/>  </output>  <opcode>/* Saw oscillator */opcode SawVco, a, kk  kfrq, kshape xin  kfrq     limit     kfrq, 0, 50000  kshape   limit     kshape, 0.001, 0.999  aout     vco2      1, kfrq, 4, kshape  xout aoutendop  </opcode></modul>