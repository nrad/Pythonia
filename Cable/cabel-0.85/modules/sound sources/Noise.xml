<?xml version="1.0" encoding="UTF-8"?><modul xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="modul.xsd" name="Noise" description="Noise generator">  <input>    <vardef name="amp" csType="k" value="1" min="0" description="Amplitude of noise signal"/>  </input>  <output>    <vardef name="out" csType="a" description="Noise output"/>  </output>  <opcode>/* Noise generator */opcode Noise, a, k  kamp xin  kamp    limit     kamp, 0, 32000  iseed   =         rnd(1)  aout    rand      kamp, iseed  xout aoutendop  </opcode></modul>