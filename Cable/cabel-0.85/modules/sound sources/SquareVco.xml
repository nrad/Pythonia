<?xml version="1.0" encoding="UTF-8"?><modul xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="modul.xsd" name="SquareVco" description="Square oscillator (amplitude 1)">  <input>    <vardef name="frq" csType="k" min="0" description="Frequency of oscillator"/>    <vardef name="pw" csType="k" min="0.001" max="0.999" value="0.5" description="Pulse width of square wave"/>  </input>  <output>    <vardef name="out" csType="a" description="Output of oscillator (amplitude 1)"/>  </output>  <opcode>/* Square oscillator */opcode SquareVco, a, kk  kfrq, kpw xin  kfrq     limit     kfrq, 0, 50000  kpw      limit     kpw, 0.001, 0.999  aout     vco2      1, kfrq, 2, kpw  xout aoutendop  </opcode></modul>